{
  "name": "stadsplan",
  "version": "0.0.1",
  "author": "Marcus Asplund <marcus@greatname.se> (@marcusasplund)",
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marcusasplund/stadsplan.git"
  },
  "bugs": {
    "url": "https://github.com/marcusasplund/stadsplan/issues"
  },
  "homepage": "https://github.com/marcusasplund/stadsplan#readme",
  "scripts": {
    "start": "standard && parcel src/index.html -p 3000 --open",
    "build": "rm -rf /release && standard && parcel build src/index.html -d build --public-url ./"
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-sw-cache": "^0.2.6",
    "standard": "*",
    "node-sass": "^4.10.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "build/"
    ]
  },
  "dependencies": {
    "leaflet": "^1.3.4",
    "leaflet-rastercoords": "^1.0.3"
  }
}
